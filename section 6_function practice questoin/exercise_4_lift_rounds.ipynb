      ],
      "source": [
        "def lift_rounds(total_people, capacity):\n",
        "    import math\n",
        "    return math.ceil(total_people / capacity)\n",
        "\n",
        "total = int(input('Total people: '))\n",
        "cap = int(input('Lift capacity: '))\n",
        "print('Rounds needed:', lift_rounds(total, cap))"
      ]
    }
